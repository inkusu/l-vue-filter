{
  "name": "luee",
  "version": "0.0.12",
  "description": "Vueの拡張",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint \"src/**/*.ts\" --fix --project tsconfig.json",
    "build": "rm -rf dist && tsc",
    "test-spec": "npx jest --coverage",
    "prepare": "npm run build && npm run lint",
    "codecov": "codecov",
    "publish": "npm version patch && git push origin master --force",
    "local-publish": "npm publish --access public && git push origin master --force"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inkusu/luee"
  },
  "keywords": [
    "typescript",
    "node",
    "vue"
  ],
  "author": {
    "name": "Wakai Yutaka",
    "email": "lnkusuin@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/inkusu/luee/issues"
  },
  "homepage": "https://github.com/inkusu/luee#readme",
  "peerDependencies": {
    "typescript": ">=2.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "in-viewport": "^3.6.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@types/jest": "25.2.3",
    "jest": "26.0.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.0.0",
    "tslint": "6.1.2",
    "tslint-config-standard": "9.0.0",
    "typescript": "3.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
